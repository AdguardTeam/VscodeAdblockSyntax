{
    "name": "adblock",
    "displayName": "Adblock Rules Language",
    "description": "VS code extension that adds support for ad blocking rules syntax.",
    "version": "0.0.2",
    "publisher": "adguard",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "adblock",
                "aliases": [
                    "adblock",
                    "adguard",
                    "ublock"
                ],
                "extensions": [
                    ".txt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "adblock",
                "scopeName": "text.adblock",
                "path": "./syntaxes/adblock.tmLanguage.json"
            }
        ]
    }
}