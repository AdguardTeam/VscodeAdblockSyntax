{
    "name": "adblock",
    "displayName": "Adblock/AdGuard/uBlock filters grammar",
    "description": "VS code extension that adds support for ad blocking rules syntax.",
    "version": "0.0.13",
    "publisher": "adguard",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ameshkov/VscodeAdblockSyntax.git"
    },
    "bugs": {
        "url": "https://github.com/ameshkov/VscodeAdblockSyntax/issues"
      },
    
    "contributes": {
        "languages": [
            {
                "id": "adblock",
                "aliases": [
                    "adblock",
                    "adguard",
                    "ublock"
                ],
                "extensions": [
                    ".txt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "adblock",
                "scopeName": "text.adblock",
                "path": "./syntaxes/adblock.tmLanguage.json"
            }
        ]
    }
}